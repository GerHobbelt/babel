{
  "name": "babel",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "make build",
    "fix": "make fix",
    "lint": "make lint",
    "precommit-orig": "lint-staged",
    "test": "make test",
    "pub": "make publish"
  },
  "devDependencies": {
    "@gerhobbelt/babel-cli": "7.0.0-beta.530005",
    "@gerhobbelt/babel-core": "7.0.0-beta.530005",
    "@gerhobbelt/babel-plugin-transform-runtime": "7.0.0-beta.530005",
    "@gerhobbelt/babel-plugin-proposal-class-properties": "7.0.0-beta.530005",
    "@gerhobbelt/babel-plugin-proposal-export-namespace-from": "7.0.0-beta.530005",
    "@gerhobbelt/babel-plugin-proposal-numeric-separator": "7.0.0-beta.530005",
    "@gerhobbelt/babel-plugin-transform-modules-commonjs": "7.0.0-beta.530005",
    "@gerhobbelt/babel-preset-env": "7.0.0-beta.530005",
    "@gerhobbelt/babel-preset-flow": "7.0.0-beta.530005",
    "@gerhobbelt/babel-register": "7.0.0-beta.530005",
    "@gerhobbelt/babel-runtime": "7.0.0-beta.530005",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-transform-charcodes": "0.1.0",
    "browserify": "16.2.2",
    "bundle-collapser": "1.3.0",
    "chalk": "2.4.1",
    "charcodes": "0.1.0",
    "derequire": "2.0.6",
    "enhanced-resolve": "4.1.0",
    "eslint": "5.3.0",
    "eslint-config-babel": "7.0.2",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-prettier": "2.6.2",
    "flow-bin": "0.78.0",
    "graceful-fs": "4.1.11",
    "gulp": "4.0.0",
    "gulp-babel": "8.0.0-beta.2",
    "gulp-filter": "5.1.0",
    "gulp-newer": "1.4.0",
    "gulp-plumber": "1.2.0",
    "gulp-rename": "1.4.0",
    "gulp-uglify": "3.0.1",
    "gulp-util": "3.0.8",
    "gulp-watch": "5.0.1",
    "husky": "0.14.3",
    "jest": "23.5.0",
    "lerna": "3.0.3",
    "lerna-changelog": "0.8.0",
    "lint-staged": "7.2.2",
    "lodash": "4.17.10",
    "merge-stream": "1.0.1",
    "output-file-sync": "2.0.1",
    "prettier": "1.14.2",
    "pump": "3.0.0",
    "rimraf": "2.6.2",
    "rollup-plugin-babel": "4.0.0-beta.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-stream": "1.24.1",
    "test262-stream": "1.2.0",
    "through2": "2.0.3",
    "uglify-js": "3.4.7",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "2.0.0",
    "webpack": "4.16.5",
    "webpack-dependency-suite": "2.4.5",
    "webpack-stream": "5.1.1",
    "yarn": "1.9.4"
  },
  "engines": {
    "node": ">= 6.9.0 <= 11.0.0-0",
    "npm": ">= 2.x <= 5.x",
    "yarn": ">=0.27.5 || >=1.0.0-20170811"
  },
  "lint-staged": {
    "*.js": [
      "eslint --format=codeframe --rulesdir='./scripts/eslint_rules'"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "packages/*/src/**/*.mjs",
      "packages/*/src/**/*.js",
      "codemods/*/src/**/*.mjs",
      "codemods/*/src/**/*.js"
    ],
    "testRegex": "./(packages|codemods)/[^/]+/test/.+\\.m?js$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/fixtures/",
      "/test/debug-fixtures/",
      "/babel-parser/test/expressions/",
      "/test/tmp/",
      "/test/__data__/",
      "/test/helpers/",
      "<rootDir>/test/warning\\.js",
      "<rootDir>/build/",
      "_browser\\.js"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/test/testSetupFile.js",
    "transformIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/packages/babel-standalone/babel(\\.min)?\\.js",
      "<rootDir>/packages/babel-preset-env-standalone/babel-preset-env(\\.min)?\\.js",
      "/test/(fixtures|tmp|__data__)/",
      "<rootDir>/(packages|codemods)/[^/]+/lib/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/packages/babel-standalone/babel(\\.min)?\\.js",
      "<rootDir>/packages/babel-preset-env-standalone/babel-preset-env(\\.min)?\\.js",
      "/test/(fixtures|tmp|__data__)/"
    ],
    "modulePathIgnorePatterns": [
      "/test/fixtures/",
      "/test/tmp/",
      "/test/__data__/",
      "<rootDir>/build/"
    ]
  }
}
